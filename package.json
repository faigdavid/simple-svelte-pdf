{
	"name": "svelte-pdf-scrollable",
	"version": "1.2.0",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "next",
		"@types/file-saver": "^2.0.5",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"autoprefixer": "^10.4.8",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.16",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.8",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0-beta.1"
	},
	"type": "module",
	"keywords": [
		"svelte",
		"pdfjs",
		"pdf",
		"component",
		"pdfjs-dist",
		"viewer",
		"sveltekit",
		"svelte-kit",
		"typescript"
	],
	"license": "MIT",
	"author": "github.com/faigdavid",
	"repository": {
		"type": "git",
		"url": "https://github.com/faigdavid/svelte-pdf-scrollable"
	},
	"dependencies": {
		"file-saver": "^2.0.5",
		"pdfjs-dist": "^2.16.105"
	}
}
